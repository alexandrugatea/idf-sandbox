<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:dp="urn:import:com.candorgrc.idfusion.sandbox.client.datapresentation">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel styleName="person-view">
		<aside class="side-controls">
			<div class="main-actions">
				<button ui:field="toggle" class="toggle-view-mode" data-tooltip="View as Table">
					<span class="icon-container">
						<span></span><span></span><span></span><span></span><span></span><span></span><span></span><span></span><span></span>
					</span>
				</button>
				<button class="open-filters" data-tooltip="Filter list">
					<i class="fa fa-filter"></i>
				</button>
				<button ui:field="fetch" class="fetch" data-tooltip="Fetch more data"><i class="fa fa-plus"></i></button>
			</div>
			<div class="secondary-actions">
				<button ui:field="reset" class="reset" data-tooltip="Reset Data Collection"><i class='fa fa-undo'></i></button>
			</div>
		</aside>
		<div class="filter-toolbar">
			<div class="filter-toolbar-content">
				<div class="form-group title">
					<select class="value form-element form-select" placeholder="Title" onclick="this.setAttribute('value', this.value);" value="">
						<option value=""></option>
						<option value="Mr.">Mr.</option>
						<option value="Mrs.">Mrs.</option>
						<option value="Ms.">Ms.</option>
					</select>
					<span class="label">Title:</span>
				</div>
				<div class="form-group first-name">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;"/>
					<span class="label">First Name:</span>
				</div>
				<div class="form-group last-name">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;"/>
					<span class="label">Last Name:</span>
				</div>
				<div class="form-group suffix">
					<select class="value form-element form-select" placeholder="&nbsp;" onclick="this.setAttribute('value', this.value);" value="">
						<option value=""></option>
						<option value="PhD">PhD.</option>
						<option value="Engineer">Engineer</option>
						<option value="Doctor">Doctor</option>
					</select>
					<span class="label">Suffix:</span>
				</div>
				<div class="form-group gender">
					<select class="value form-element form-select" placeholder="&nbsp;" onclick="this.setAttribute('value', this.value);" value="">
						<option value=""></option>
						<option value="m">Male</option>
						<option value="f">Female</option>
					</select>
					<span class="label">Gender:</span>
				</div>
				<div class="form-group race">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Race:</span>
				</div>
				<div class="form-group language">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Language:</span>
				</div>
				<div class="form-group university">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">University:</span>
				</div>
				<div class="form-group buzzword">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Buzzword:</span>
				</div>
				<div class="form-group email">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">E-Mail:</span>
				</div>
				<div class="form-group job-title">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Job Title:</span>
				</div>
				<div class="form-group linkedin-skill">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">LinkedinIn Skill</span>
				</div>
				<div class="form-group company">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Company:</span>
				</div>
				<div class="form-group dept">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Department:</span>
				</div>
				<div class="form-group ein">
					<input type="text" class="value form-element form-input" placeholder="&nbsp;" />
					<span class="label">Employer Identification Number:</span>
				</div>
				<div class="form-group date date-start">
					<input type="text" id="start" class="form-element form-input" min="2018-01-01" max="2018-12-31" value="" placeholder="&nbsp;"
						onclick="this.setAttribute('value', '2018-09-29');this.setAttribute('type', 'date');"/>
					<label for="start" class="label">Date Start:</label>
				</div>
				<div class="form-group date date-end">
					<input type="text" id="end" class="form-element form-input" min="2018-01-01" max="2018-12-31" value="" placeholder="&nbsp;"
						onclick="this.setAttribute('value', '2018-09-29');this.setAttribute('type', 'date');"/>
					<label for="end" class="label">Date End:</label>
				</div>
				<div class="form-group actions-toolbar">
					<button ui:field="apply" class="apply-filters">Apply Filters</button>
				</div>
			</div>
		</div>
		<section class="content">
			<dp:PersonCellList ui:field="personList"
				styleName="grid"> 

			</dp:PersonCellList>

		</section>
		<footer class="copyright"> &copy; Alexandru Gatea. Style rights reserved! </footer>
	</g:HTMLPanel>
</ui:UiBinder> 